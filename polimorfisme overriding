class Akun:
    def __init__(self, nama):
        self.nama = nama

    def akses(self):
        print("Akun biasa hanya bisa melihat informasi")


class Premium(Akun):
    def __init__(self, nama):
        super().__init__(nama)

    def akses(self):
        print(f"Akun premium milik {self.nama}")
        print("Bisa melihat, mengunduh, dan mengedit data")


# objek
akun1 = Akun("Rian")
akun2 = Premium("Dewa")

# pemanggilan method
akun1.akses()
print("-----")
akun2.akses()
